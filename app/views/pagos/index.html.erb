<p id="notice"><%= notice %></p>

<h1>Listing Pagos</h1>

<table>
  <thead>
    <tr>
      <th>Concepto</th>
      <th>Cantidad</th>
      <th>Jugador</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pagos.each do |pago| %>
      <tr>
          <td><%= pago.created_at.to_date %></td>
          <td><%= pago.concepto %></td>
          <td><%= concep.tarifa %></td>
          <td><%= pago.cantidad %></td>
          <td><%= concep.tarifa - pago.cantidad %></td>
      </tr>
    <% end %>
  </tbody>
</table>
