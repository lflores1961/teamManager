<p id="notice"><%= notice %></p>

<h1>Listing Pagos</h1>

<table>
  <thead>
    <tr>
      <th>Concepto</th>
      <th>Cantidad</th>
      <th>Jugador</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pagos.each do |pago| %>
      <tr>
        <td><%= pago.concepto %></td>
        <td><%= pago.cantidad %></td>
        <td><%= pago.jugador %></td>
    <% end %>
  </tbody>
</table>
